{
  "manifest_version": 3,
  "name": "NoteMaker",
  "version": "1.0.0",
  "description": "create and download your notes",
  "icons": {
    "128": "logo192.png",
    "48": "logo192.png",
    "16": "logo192.png"
  },
  "permissions": ["tabs"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["newnotes.js"]
    }
  ]
}
